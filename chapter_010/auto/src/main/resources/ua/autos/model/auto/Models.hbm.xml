<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="ua.autos.model.auto.Models" table="models">
        <id name="id" column="id_model">
            <generator class="identity"/>
        </id>
        <property name="values" column="values" length="255" not-null="true"/>
        <many-to-one name="brands" class="ua.autos.model.auto.Brands"
                     column="brand_id" not-null="false"/>
    </class>
</hibernate-mapping>
