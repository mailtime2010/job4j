<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="ua.autos.model.auto.Auto" table="auto">
        <id name="id" column="id_auto">
            <generator class="identity"/>
        </id>
        <many-to-one name="brand" class="ua.autos.model.auto.Brands" lazy="false"
                     fetch="select" column="brand_id" not-null="false" unique="true" cascade="none"/>
        <many-to-one name="model" class="ua.autos.model.auto.Models" lazy="false"
                     fetch="select" column="model_id" not-null="false" unique="true" cascade="none"/>
        <many-to-one name="color" class="ua.autos.model.auto.Colors" lazy="false"
                     fetch="select" column="color_id" not-null="false" unique="true" cascade="none"/>
        <many-to-one name="engine" class="ua.autos.model.auto.Engines" lazy="false"
                     fetch="select" column="engine_id" not-null="false" unique="true" cascade="none"/>
        <many-to-one name="year" class="ua.autos.model.auto.Years" lazy="false"
                     fetch="select" column="year_id" not-null="false" unique="true" cascade="none"/>
    </class>
</hibernate-mapping>
