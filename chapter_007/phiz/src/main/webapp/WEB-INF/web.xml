<?xml version="1.0" encoding="ISO-8859-1" ?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/j2ee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
         http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
         version="2.4">
    <welcome-file-list>
        <welcome-file>/WEB-INF/jsp/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>UserGateServlet</servlet-name>
        <servlet-class>web.present.UserLoginGateServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserGateServlet</servlet-name>
        <url-pattern>/gate</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>web.present.UserLoginGateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserListServlet</servlet-name>
        <servlet-class>web.present.UserListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserListServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserAddServlet</servlet-name>
        <servlet-class>web.present.UserAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserAddServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserEditServlet</servlet-name>
        <servlet-class>web.present.UserEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserEditServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserViewServlet</servlet-name>
        <servlet-class>web.present.UserViewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserViewServlet</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserDeleteServlet</servlet-name>
        <servlet-class>web.present.UserDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserDeleteServlet</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserClearServlet</servlet-name>
        <servlet-class>web.present.UserClearServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserClearServlet</servlet-name>
        <url-pattern>/clear</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserUploadServlet</servlet-name>
        <servlet-class>web.present.UserUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserUploadServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserLoadServlet</servlet-name>
        <servlet-class>web.present.UserLoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoadServlet</servlet-name>
        <url-pattern>/load</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserLogOutServlet</servlet-name>
        <servlet-class>web.present.UserLogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogOutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>User444Servlet</servlet-name>
        <servlet-class>web.present.User444Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User444Servlet</servlet-name>
        <url-pattern>/444</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>User404Servlet</servlet-name>
        <servlet-class>web.present.User404Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User404Servlet</servlet-name>
        <url-pattern>/404</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/404</location>
    </error-page>
</web-app>
